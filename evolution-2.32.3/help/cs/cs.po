msgstr "ctrl+j"

#: C/evolution.xml:3929(para)
msgid "Week"
msgstr "Týden"

#: C/evolution.xml:3932(para)
msgid "control+k"
msgstr "ctrl+k"

#: C/evolution.xml:3937(para)
msgid "Month"
msgstr "Měsíc"

